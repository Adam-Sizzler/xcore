[Unit]
Description=Reverse Proxy Service
After=network.target

[Service]
User=root
Group=root
ExecStart=/usr/local/reverse_proxy/reverse/go run main.go -dns=true
WorkingDirectory=/usr/local/reverse_proxy/reverse
Restart=always
RestartSec=5
KillSignal=SIGTERM
TimeoutStopSec=10

[Install]
WantedBy=multi-user.target
